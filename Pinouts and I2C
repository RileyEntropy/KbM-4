const int LEFT_MOUSE_PIN = 15; //Trigger
const int R_KEY_PIN = 14; //Mag Release
const int SHIFT_PIN = 16; //Foregrip Button 1
const int SPACEBAR_PIN = 9; // Foregrip Button 2
const int LED_PIN = 13;
const int SPEED_REDUCE_PIN = 10; //foregrip button 3

const int PRESSURE_SENSOR_PIN = A3;  // This pin only controls MMB // Thin-film pressure sensor


const int xPin1 = A2; // Joystick pin definition for WASD
const int yPin1 = A1;
